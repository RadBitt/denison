  "dependencies": {
    "autoprefixer-stylus": "^0.14.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "firebase": "^5.5.5",
    "nodemon": "^1.18.4",
    "prop-types": "15.6.0",
    "re-base": "^4.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.5.0",
    "serve": "^10.0.2",
